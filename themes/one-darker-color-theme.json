  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": "#d19a66",
    "property": "#ef596f",
    "variable.defaultLibrary": "#e5c07b"
  },
  "tokenColors": [
    {
      "name": "Chalky",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "keyword.other.type",
        "punctuation.definition.annotation",
        "storage.modifier.import",
        "storage.modifier.package",
        "storage.type.annotation",
        "storage.type.built-in",
        "storage.type.generic",
        "storage.type.java",
        "storage.type.groovy",
        "storage.type.primitive",
        "support.class",
        "support.other.namespace",
        "support.type",
        "variable.language.this"
      ],
      "settings": { "foreground": "#e5c07b" }
    },
    {
      "name": "Coral",
      "scope": [
        "constant.other.character-class",
        "entity.name.tag",
        "heading",
        "meta.object-literal.key",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.list.end.markdown",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded",
        "support.type.property-name",
        "variable.object.property",
        "variable.other.enummember"
      ],
      "settings": { "foreground": "#ef596f" }
    },
    {
      "name": "Cyan",
      "scope": [
        "constant.character.escape",
        "keyword.operator",
        "markup.underline.link",
        "string.regexp",
        "string.url"
      ],
      "settings": { "foreground": "#2bbac5" }
    },
    {
      "name": "Malibu",
      "scope": [
        "entity.name.function",
        "entity.other.attribute-name.id.css",
        "meta.function-call.generic",
        "string.other.link",
        "support.function",
        "variable.language.super"
      ],
      "settings": { "foreground": "#61afef" }
    },
    {
      "name": "Ivory",
      "scope": [
        "meta.brace",
        "punctuation.accessor",
        "punctuation.definition.block",
        "punctuation.separator",
        "support.type.property-name.css"
      ],
      "settings": { "foreground": "#bbbbbb" }
    },
    {
      "name": "Sage",
      "scope": [
        "markup.inline",
        "markup.quote",
        "source.ini",
        "string.other.link.description",
        "string"
      ],
      "settings": { "foreground": "#89ca78" }
    },
    {
      "name": "Stone",
      "scope": ["comment"],
      "settings": { "foreground": "#5c6370" }
    },
    {
      "name": "Violet",
      "scope": [
        "keyword.operator.new",
        "keyword",
        "markup.italic",
        "punctuation.definition.block.tag",
        "storage.modifier",
        "storage.type"
      ],
      "settings": { "foreground": "#d55fde" }
    },
    {
      "name": "Whiskey",
      "scope": [
        "constant",
        "entity.other.attribute-name",
        "keyword.operator.quantifier.regexp",
        "markup.bold",
        "support.constant",
        "variable.other.constant",
        "variable.parameter"
      ],
      "settings": { "foreground": "#d19a66" }
    },
    {
      "name": "Always italic",
      "scope": ["markup.quote", "markup.italic"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Always bold",
      "scope": ["heading", "markup.bold"],
      "settings": { "fontStyle": "bold" }
    },
    {
      "name": "One Dark italic",
      "scope": [
        "comment",
        "entity.other.attribute-name",
        "keyword",
        "markup.underline.link",
        "storage.modifier",
        "storage.type",
        "string.url",
        "variable.language.super",
        "variable.language.this"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "One Dark italic reset",
      "scope": [
        "keyword.operator",
        "keyword.other.type",
        "storage.modifier.import",
        "storage.modifier.package",
        "storage.type.built-in",
        "storage.type.function.arrow",
        "storage.type.generic",
        "storage.type.java",
        "storage.type.primitive"
      ],
      "settings": {
        "fontStyle": ""
      }
    }
  ]
}
